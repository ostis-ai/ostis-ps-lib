sc_node_not_relation
    -> atomic_logical_formula;
    -> concept_message;
    -> concept_svo_triple;
    -> concept_svo_triple_element;
    -> not_enough_specified_essence;
    -> concept_message_about_entity_relation_to_entity;;

sc_node_norole_relation
    -> nrel_svo_triples;
    -> nrel_implication;
    -> _relation_value;;

sc_node_role_relation
    -> rrel_svo_verb;
    -> rrel_svo_triple_element_values;
    -> rrel_main_key_sc_element;;

sc_node_tuple
    -> _tuple;;

@rule_condition = [*
    _message
        _<- concept_message;
        _<- concept_message_about_entity_relation_to_entity;;
    _message _=> nrel_svo_triples:: _tuple;;
    _svo_triple
        _<- concept_svo_triple;
        _<- _tuple;
        _-> rrel_svo_verb:: _element;;
    _element _<- concept_svo_triple_element;;
*];;

@rule_result = [*
    _element _-> rrel_svo_triple_element_values:: _values_set;;
    _relation_value
        _<- _values_set;
        _<- relation;;
*];;

atomic_logical_formula
    -> @rule_condition;
    -> @rule_result;;


//actual name of the rule is lr_svo_triple_message_about_entity_relation_to_entity_verb_elements_value_generation but on some machines it melds with lr_svo_triple_message_about_entity_action_with_entity_verb_elements_value_generation despite different system identifiers.

@rule_implication_arc = (@rule_condition => @rule_result);;
@rule_implication_arc
    <- nrel_implication;
    <- rrel_main_key_sc_element: lr_svo_triple_message_about_entity_relation_to_entity_verb_elements_value;;

lr_svo_triple_message_about_entity_relation_to_entity_verb_elements_value
    <- concept_svo_triple_element_value_generation_rule;
    <- concept_svo_triple_element_value_generation_rule_class_by_priority_1;;

