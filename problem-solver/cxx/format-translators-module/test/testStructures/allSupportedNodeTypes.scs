@var_node_link_class = _[sample text];;

@const_node_link => nrel_system_identifier: [const_node_link];;
@const_node_link_class => nrel_system_identifier: [const_node_link_class];;
@var_node_link => nrel_system_identifier: [var_node_link];;
@var_node_link_class => nrel_system_identifier: [var_node_link_class];;

sc_link
    -> @const_node_link;
    -> @var_node_link;;

sc_link_class
    -> @const_node_link_class;
    -> @var_node_link_class;;

sc_node_tuple
    -> const_node_tuple;
    -> _var_node_tuple;;

sc_node_structure
    -> const_node_structure;
    -> _var_node_structure;;

sc_node_role_relation
